			<java-type>BLOB</java-type>
			<db-type>BLOB SUB_TYPE 0</db-type>
		</type-mapping>
		<type-mapping>
			<java-type>CLOB</java-type>
			<db-type>BLOB SUB_TYPE 1</db-type>
		</type-mapping>
		<images>%context%/%expresso-dir%/images</images>
